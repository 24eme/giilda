#!/bin/bash

. bin/config.inc

if ! test "$1"; then
    . bin/download_redmine_file https://clients.actualys.com/attachments/download/4046/VINSDELOIRE_DATA.zip /tmp/VINSDELOIRE_DATA.zip
fi

SYMFODIR=$(pwd);

LOGDATE=$SYMFODIR/$(date +%Y%m%d%H%M%S_import_civp_data.log)

{
rm -rf /tmp/VINSDELOIRE_DATA 2>/dev/null ; mkdir /tmp/VINSDELOIRE_DATA 2> /dev/null ; cd /tmp/VINSDELOIRE_DATA
unzip /tmp/VINSDELOIRE_DATA.zip

DATADIR=$(pwd);

echo "Import des Ã©tablissements";

cd $SYMFODIR

php symfony import:etablissement --trace /tmp/VINSDELOIRE_DATA/etablissements.csv

}
