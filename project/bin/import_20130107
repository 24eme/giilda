#!/bin/bash

bash bin/delete_from_view.sh "http://localhost:5984/vinsdeloire/_design/compte/_view/all"
bash bin/delete_from_view.sh "http://localhost:5984/vinsdeloire/_design/etablissement/_view/all?reduce=false"
bash bin/delete_from_view.sh "http://localhost:5984/vinsdeloire/_design/societe/_view/all"
bash bin/delete_from_view.sh "http://localhost:5984/vinsdeloire/_design/ds/_view/history"
bash bin/delete_from_view.sh "http://localhost:5984/vinsdeloire/_design/sv12/_view/all"
bash bin/delete_from_view.sh "http://localhost:5984/vinsdeloire/_design/vrac/_view/history"

bash bin/import_contacts

echo "Import des vracs et sv12";

bash bin/import_data_vrac

php symfony import:vrac-sv12 --trace /tmp/VINSDELOIRE_DATA/vracs.csv

echo "Import des DS"

bash bin/import_data_ds

php symfony import:ds --trace /tmp/VINSDELOIRE_DATA/ds.csv