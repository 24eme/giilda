#!/bin/bash

. bin/config.inc
. bin/import_functions.inc


if ! test "$1"; then
    echo "Le paramètre d'import doit être indiqué";
    exit 1;
fi

if ! test "$2"; then
    echo "L'environment doit être indiqué";
    exit 1;
fi

IMPORT=$1
ENV=$2

echo "Lancement de la creation des alertes"

php symfony generate:alertes_creations --import=$IMPORT --env=$ENV --trace

echo "FIN de la creation des alertes"
echo ""
echo "Lancement de la mise à jour des alertes"

php symfony generate:alertes_updates --import=$IMPORT --env=$ENV --trace

echo "FIN de la mise à jour des alertes"
