DATADIR=/tmp/VINSDELOIRE_DATA

echo "conversion des chais pour Ã©viter les doublons"

cd $DATADIR
bash bin/convert_chais
cd -

echo "Import Societe"

iconv -f ISO88591 -t UTF8 $DATADIR/cipar.csv | sort -t ';' -k 2 > $DATADIR/tmp/cipar.utf8.csv

php symfony import:societe $DATADIR/tmp/cipar.utf8.csv

echo "Import Etablissement"

iconv -f ISO88591 -t UTF8 $DATADIR/cicav.csv | sort -t ';' -k 2 > $DATADIR/tmp/cicav.utf8.csv
join -t ';' -a 1 -1 2 -2 2 $DATADIR/tmp/cipar.utf8.csv $DATADIR/tmp/cicav.utf8.csv > $DATADIR/etablissement.csv

php symfony import:etablissement $DATADIR/tmp/cipar.utf8.csv



